        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(window_draw)
{
    var mx = device_mouse_x_to_gui(0)
        my = device_mouse_y_to_gui(0);
    if(mouse_check_button_pressed(mb_left))
    {
        if(point_in_rectangle(mx,my,x,y,x+w,y+bar_height+frame_width*2))
        {
            drag = 1;
            drag_x = mx;
            drag_y = my;
        }
        if(point_in_rectangle(mx,my,x+button_close_x,y+button_close_y,x+button_close_w,y+button_close_h))
        {
            window_show(0);
        }
    }
    if(mouse_check_button_released(mb_left))
    {
        drag = 0;
        drag_x = 0;
        drag_y = 0;
    }
    x+=(mx-drag_x)*drag;
    y+=(my-drag_y)*drag;
    drag_x = mx;
    drag_y = my;
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
