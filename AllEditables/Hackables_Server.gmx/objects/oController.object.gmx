for(var i=0; i&lt;ds_grid_width(global.maps_users); i++)
{
    for(var j=0; j&lt;ds_grid_height(global.maps_users); j++)
    {
        ds_grid_set(global.maps_users,i,j, ds_list_create());
        //This list should hold object id's
    }
}
