if(os_type==os_windows)
{
    var v = (os_version div 65536) + (os_version mod 65536)*0.1;
    if(v &gt;= 6.1){
        APPDATA = environment_get_variable("LOCALAPPDATA")+"\"+game_project_name;
    }else if(v &gt;= 6.0){
        APPDATA = environment_get_variable("APPDATA")+"\"+game_project_name;
    }else{
        APPDATA = working_directory;
    }
    logAdd("APPDATA = "+string(APPDATA));
}else{
    show_message("This program can only run on windows machine");
    game_end();
    exit;
}
